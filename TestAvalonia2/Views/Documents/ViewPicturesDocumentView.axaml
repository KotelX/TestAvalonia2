<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:vm="using:TestAvalonia2.ViewModels.Documents"
             xmlns:views="using:TestAvalonia2.Views.Documents"
             x:Class="TestAvalonia2.Views.Documents.ViewPicturesDocumentView"
             x:DataType="vm:ViewPicturesDocumentViewModel">
    <Grid Focusable="True">
    <Image Source="{Binding ImageSource}"/>
  </Grid>
</UserControl>
